
@{
    ViewBag.Title = "_LoginLayout";
}


<style>
    body {
        margin-left: 0px;
        margin-top: 0px;
        margin-right: 0px;
        margin-bottom: 0px;
        font-family: Helvetica, Arial, sans-serif;
        font-size: 12px;
        background-position: center;
        background-image: url(/Content/img/spa.gif);
        background-size: 100% 100%;
        background-repeat: no-repeat;
        background-attachment: fixed;
    }

    .footerSection {
        width: 100%;
        height: 70px;
        background-color: #2E75BC;
        display: flex;
    }

    .body-content {
        height: 350px;
        /*height:63%;*/
    }

    .footerSection img {
        float: right;
        float: right;
        height: 60px;
    }

    .footerdesign {
        width: 60%;
        float: left;
        font-size: 15px;
        color: white;
        font-style: italic;
        font-weight: 400;
    }

        .footerdesign img {
            height: 60px;
            width: 789px;
        }

    .footerimg {
        width: 40%;
        float: right;
        background-color: #F2F5E5;
        width: 40%;
        margin-top: 15px;
        clip-path: polygon(0% 100%, 100% 100%, calc(100% - 0px) 0%, 0% 0%, calc(100% - 40px) 0%, 10% 0%);
    }
</style>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/Content/img/square_banner.png" type="image/png">
    <title>SPA MIS</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/Content/bootstrap/dist/css/bootstrap.min.css">
