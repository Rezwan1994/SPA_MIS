    <style>
        .row {
            margin: 0;
        }

        .blur {
            /* overflow: hidden; */
            -webkit-filter: blur(13px);
            -moz-filter: blur(13px);
            -o-filter: blur(13px);
            -ms-filter: blur(13px);
            filter: blur(13px);
        }

        .LoaderStyle {
            margin: 0px;
            padding: 0px;
            position: fixed;
            right: -52%;
            top: 40%;
            width: 100%;
            height: 100%;
            z-index: 30001;
            opacity: .8;
            filter: alpha(opacity=70);
        }
    </style>


    <script src="~/Content/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/jquery-ui-1.12.1/jquery-ui-1.12.1.min.js"></script>
