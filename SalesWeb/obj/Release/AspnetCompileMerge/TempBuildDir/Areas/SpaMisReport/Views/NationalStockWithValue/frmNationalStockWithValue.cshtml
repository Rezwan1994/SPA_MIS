
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Areas/SpaMisReport/NgCode/frmNationalStockWithValue.js"></script>
<div ng-app="myApp" ng-controller="NationalStockWithValueCtrl">
    <div class="loader LoaderStyle" ng-cloak data-loading>
        <img src="~/Content/img/loader.gif" width="100" height="100" style="top: 50%; left: 50%;" />
    </div>
    <div class="form-horizontal" ng-form="frmNationalStockWithValue">
        <section class="content">
            <div class="row">
                <div class="col-md-12">
                    <div class="box box-warning">
                        <div class="box-header with-border">
                            <div class="form-group">
                                <div class="col-md-5">

                                    <div>
                                        <h2 class="box-title">
                                            <strong ng-cloak>{{FormTitle}}</strong>
                                        </h2>

                                    </div>

                                </div>

                                    <div class="col-md-7">
                                        <div class="text-right  m-t-10">
                                            <button type="button" class="btn btn-embossed btn-success" ng-click="GetNationalStockWithValue()" ng-class="{'btn-transparent': frmNationalStockWithValue.$invalid }" ng-disabled="frmNationalStockWithValue.$invalid"><i class="fa fa-search"></i>View</button>
                                            <button type="button" class="btn btn-embossed btn-danger" ng-click="Reset()"><i class="fa fa-refresh"></i>&nbsp Reset</button>
                                        </div>
                                    </div>

                            </div>
                        </div>
                        <div class="box-body">
                            <form role="form">
                                <div class="row">



                                    <div id="gridNationalStockWithValue" ui-grid="gridNationalStockWithValue" class="grid" style="height: 500px; width: 100%;" ui-grid-auto-resize ui-grid-resize-columns ui-grid-exporter></div>

                                </div>
                            </form>

                        </div>

                    </div>
                </div>

            </div>
        </section>
    </div>
</div>
<script>
    $('.datepicker').datepicker({
        todayBtn: "linked",
        autoclose: true,
        todayHighlight: true,
        format: 'dd/mm/yyyy',
        changeMonth: true,
        changeYear: true
    })

</script>
